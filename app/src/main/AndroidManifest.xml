<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hopjs.filmcinema">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UI.MyCritic"/>
        <activity android:name=".UI.TicketRecord"/>
        <activity android:name=".UI.About"/>
        <activity android:name=".UI.CinemaChoose"/>
        <activity android:name=".UI.CinemaDetail"/>
        <activity android:name=".UI.Collection"/>
        <activity android:name=".UI.Confirm"/>
        <activity android:name=".UI.FilmDetail"/>
        <activity android:name=".UI.Register"/>
        <activity android:name=".UI.Help"/>
        <activity android:name=".UI.Question"/>
        <activity android:name=".UI.Critic"/>
        <activity android:name=".UI.Search"/>
        <activity android:name=".UI.SeatChoose"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>